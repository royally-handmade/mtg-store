{
  "name": "mtg-marketplace-backend",
  "version": "1.0.0",
  "description": "MTG Marketplace Backend API - Express.js server with Supabase integration",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.mjs --fix",
    "lint:check": "eslint . --ext .js,.mjs",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "mtg",
    "magic-the-gathering",
    "marketplace",
    "tcg",
    "cards",
    "express",
    "supabase",
    "ecommerce"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@easypost/api": "^7.5.1",
    "@supabase/supabase-js": "^2.43.1",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "rate-limiter-flexible": "^5.0.3",
    "sharp": "^0.33.3",
    "stripe": "^15.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=20.6.1",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mtg-marketplace-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/mtg-marketplace-backend/issues"
  },
  "homepage": "https://github.com/yourusername/mtg-marketplace-backend#readme",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "uploads/*",
      "logs/*"
    ],
    "ext": "js,mjs,json",
    "delay": "1000"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".js"
    ],
    "globals": {
      "NODE_OPTIONS": "--experimental-vm-modules"
    },
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "routes/**/*.js",
      "middleware/**/*.js",
      "services/**/*.js",
      "!**/__tests__/**",
      "!**/node_modules/**"
    ]
  }
}
